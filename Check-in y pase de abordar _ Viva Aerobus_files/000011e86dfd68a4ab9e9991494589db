
(function() {

    var sessionSeed = (function() {
       if(localStorage && localStorage.getItem && localStorage.setItem) {
         var currentSeed = localStorage.getItem("FPSS")
         if (!currentSeed) {
           currentSeed = btoa(Math.random().toString());
           localStorage.setItem("FPSS",currentSeed)

         }
         return currentSeed;
       }
    })()

	// Begin by creating hidden Iframe where we will execute our entire context, accessing window.top explicitly
	// All of this activity must happen only after onReady to avoid unexpected effects on the DOM based on deployment code position

	/** The following section was adapted from https://github.com/airportyh/onready/blob/master/onready.js **/
	var onReady = (function(){
		var addLoadListener;
		var removeLoadListener;
		if (window.addEventListener){
			addLoadListener = function(func){
				window.addEventListener('DOMContentLoaded', func, false);
				window.addEventListener('load', func, false);
			}
			removeLoadListener = function(func){
				window.removeEventListener('DOMContentLoaded', func, false);
				window.removeEventListener('load', func, false);
			}
		}else if (document.attachEvent){
			addLoadListener = function(func){
				document.attachEvent('onreadystatechange', func);
				document.attachEvent('load', func);
			}
			removeLoadListener = function(func){
				document.detachEvent('onreadystatechange', func);
				document.detachEvent('load', func);
			}
		}

		var callbacks = null;
		var done =  document.attachEvent ? document.readyState === "complete" : document.readyState !== "loading";
		function __onReady(){
			done = true;
			removeLoadListener(__onReady);
			if (!callbacks) return;
			for (var i = 0; i < callbacks.length; i++){
				callbacks[i]();
			}
			callbacks = null;
		}
		function OnReady(func){
			if (done){
				func();
				return;
			}
			if (!callbacks){
				callbacks = [];
				addLoadListener(__onReady);
			}
			callbacks.push(func);
		}
		return OnReady;
	})();


