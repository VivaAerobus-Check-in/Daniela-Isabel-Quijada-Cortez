options.eventsCallback = function(eventType,data) {
  try {
    switch (eventType.split(':')[1] || false) {
      case "paymentok":
        gtag('event', 'conversion', {
          'send_to': 'AW-344005961/MP26COK6wcsCEMm6hKQB',
          'value': data.price,
          'currency': data.currency,
          'transaction_id': '',
          'event_callback': () => {
          }
        });
        //fbq('track', 'Schedule');
        //fbq('track', 'AddPaymentInfo');
        fbq('track', 'Purchase', {value: data.price, currency: data.currency});
        break;
      case "loadpayment":
        fbq('track', 'InitiateCheckout');
        break;
      case "search":
      case "list":
        fbq('track', 'Search');
        break;
      case "view":
        fbq('track', 'ViewContent');
        break;
      case "booking":
        fbq('track', 'AddToCart');
        break;
      case "thanks":
        break;
      default:
        break;
    }

    const getFacilities = (q) => {
      let facilities = []

      AllInclusive: "false"
      Breakfast: "false"
      FullBoard: "false"
      HalfBoard: "false"
      None: "false"


      q["24Hours"] && facilities.push("24Hours")
      q["Accessible"] && facilities.push("Accessible")
      q["AirConditioning"] && facilities.push("AirConditioning")
      q["BabySitting"] && facilities.push("BabySitting")
      q["BaggageStorage"] && facilities.push("BaggageStorage")
      q["Bar"] && facilities.push("Bar")
      q["Beach"] && facilities.push("Beach")
      q["BicycleRental"] && facilities.push("BicycleRental")
      q["BicycleStorage"] && facilities.push("BicycleStorage")
      q["ConferenceCenter"] && facilities.push("ConferenceCenter")
      q["Fitness"] && facilities.push("Fitness")
      q["FreeWiredInternet"] && facilities.push("FreeWiredInternet")
      q["GayFriendly"] && facilities.push("GayFriendly")
      q["Gym"] && facilities.push("Gym")
      q["Heating"] && facilities.push("Heating")
      q["LaundryService"] && facilities.push("LaundryService")
      q["Lift"] && facilities.push("Lift")
      q["Parking"] && facilities.push("Parking")
      q["PetsAllowed"] && facilities.push("PetsAllowed")
      q["SafeBox"] && facilities.push("SafeBox")
      q["Sauna"] && facilities.push("Sauna")
      q["SwimmingPool"] && facilities.push("SwimmingPool")
      q["WiredInternet"] && facilities.push("WiredInternet")

      return facilities;
    }

    const getBoard = (q) => {
      let board = []

      q["HalfBoard"] === "true" && board.push("HalfBoard")
      q["FullBoard"] === "true" && board.push("FullBoard")
      q["Breakfast"] === "true" && board.push("Breakfast")
      q["AllInclusive"] === "true" && board.push("AllInclusive")
      q["None"] === "true" && board.push("RoomOnly")

      return board;
    }

    const getPassengersTypes = (query) => {
      return {
        adt: query.adultCount,
        chd: (typeof (query.childAges) == 'string' ? query.childAges.split(',') : query.childAges).filter(i => i >= 2).length,
        inf: (typeof (query.childAges) == 'string' ? query.childAges.split(',') : query.childAges).filter(i => i < 2).length
      }
    }

    const getScore = (query) => {
      let start = 5;
      let end = 1
      if (query["ratingPoor"] === `true`) {
        start = 1;
      } else if (query["ratingFair"] === `true`) {
        start = 2;
      } else if (query["ratingGood"] === `true`) {
        start = 3;
      } else if (query["ratingVeryGood"] === `true`) {
        start = 4;
      }

      if (query["ratingTop"] === `true`) {
        end = 5;
      } else if (query["ratingVeryGood"] === `true`) {
        end = 4;
      } else if (query["ratingGood"] === `true`) {
        end = 3;
      } else if (query["ratingFair"] === `true`) {
        end = 2;
      }
      return [start, end];
    }

    const eventDomain = eventType.split(':')[0]
