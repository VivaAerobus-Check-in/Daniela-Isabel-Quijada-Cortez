//v11.55

window.top['dataLayer'] = window.top.dataLayer || []

var api = BidFlyerContext.api
// Report bidflyer suburl change
api.require(['lib/eventbus'], function(eventBus) {
    eventBus.on('client:notify', function(type, data) {
        if (type == 'clientState' && data.path) {
            try {

                window.top.dataLayer.push({ 'event': 'BidFlyerPage', 'infotype': 'URL', 'infoelement': data.path })
            } catch (e) {
                console.log(e)
            }

        }
    })
})


api.require(['underscore', 'lib/eventbus', 'backbone', 'moment-with-locales'], function(_, eventBus, Backbone, moment) {
    var windowTop = window.top
    var listener = {}
    _.extend(listener, Backbone.Events)

    listener.listenTo(eventBus, 'reportevent', function(url) {
        try {
            if (url == '/api/event/popupLaunched') {
                windowTop.dataLayer.push({
                    event: 'BidFlyer',
                    infotype: 'PopupRoute',
                    infoelement: 'DisplayedPopup'
                })
            }
            if (url == '/api/event/popupClick') {
                windowTop.dataLayer.push({
                    event: 'BidFlyer',
                    infotype: 'PopupRoute',
                    infoelement: 'DetailsPopup'
                })
            }
            if (url == '/api/event/popupClosed') {
                windowTop.dataLayer.push({
                    event: 'BidFlyer',
                    infotype: 'PopupRoute',
                    infoelement: 'ClosePopup'
                })
            }

        } catch (e) {
            console.log(e)
        }
    })
})

api.require(['jquery', 'moment-with-locales', 'lib/eventbus'], function($, moment, eventBus) {
    var windowTop = window.top
    try {
        api.exitIntent = function() {
        }

        //New Flow
        console.info('Wow New VivaAerobus!')

        const newHandle = (price) => {
            try {
                let intentData = {}
                const search = JSON.parse(window.top.localStorage.getItem('viva-flights-search-history'))[0].content
                intentData.outboundDate = moment(search.startDate).format('YYYY-MM-DD')

                if (typeof (search['endDate']) != 'undefined') //If roundtrip
                    intentData.inboundDate = moment(search.endDate).format('YYYY-MM-DD')

                intentData.price = Number(price || 0)
                intentData.currency = windowTop.localStorage.getItem('viva-user-currency')
                intentData.from = search.origin.code
                intentData.to = search.destination.code
                intentData.stage = 'flights'
                intentData.numberOfTickets = search.passengers.adults + search.passengers.kids + search.passengers.babies

                // if (intentData.stage == 'passengers') {
                //     api.releasePopup(); //Disable popup if passengers stage reached
                // }

                if (price) {
                    console.log('handling ', intentData)
                    api.setFlightSearch(intentData)
                } else {
                    intentData.stage = 'fares'
                    eventBus.trigger('reportevent', '/api/event/flightSearch', intentData)
                }
            } catch (e) {
                console.log('handle flight exception:', e)
            }
        }

        newHandle()

        const priceReadyInterval = setInterval(function() {
            try {
                const totalPrice = parseInt($('.cost-summary .movile-price', document).text().replace(/[\ ,]/g, ''))
                if (totalPrice) {
                    console.log('Detected Total price of ' + (totalPrice))
                    newHandle(totalPrice)
                    clearInterval(priceReadyInterval)
                }
            } catch (e) {
            }

        }, 500)





        if(windowTop.location.href.indexOf('calendario') > 10){
            const urlParams = new URLSearchParams(window.location.search);
            const launchCalendar = ()=>{
                const container = $('#Bid-CalendarContainer', document)
                if(container.length>0){
                    container.css("height",(container.width() > 700?"1120px":"470px"))
                    const tags = ["ZeroFare","Discounted"]
                    const from = urlParams.get('from') || "CUN"
                    const to = urlParams.get('to') || "MEX"
                    const adultCount = parseInt(urlParams.get('adultCount')) || 1;
                    const childCount = parseInt(urlParams.get('childCount')) || 0;//(parseInt(urlParams.get('kids')) || 0) + (parseInt(urlParams.get('infants')) || 0);
                    const childAges = parseInt(urlParams.get('childAges')) || [];//new Array(parseInt(urlParams.get('kids')) || 0).fill(10).concat(new Array(parseInt(urlParams.get('infants')) || 0).fill(1));
                    const redirectBaseUrlPath = "https://www.vivaaerobus.com/bidflyer/"//Core.PublicAPI.getHostBasePath(true)
                    const rooms = urlParams.get('roomCount') || 1
                    api.showCalendarWidget(container[0],{
                        from,
                        to,
                        adultCount,
                        childCount,
                        childAges,
                        redirectBaseUrlPath,
                        rooms,
                        tags
                    })
                }
                else{
                    windowTop.setTimeout(launchCalendar, 500)
                }
            }
            launchCalendar()
        }

    } catch (e) {
        console.log(e);
    }


});


api.require(['jquery', 'moment-with-locales', 'lib/eventbus','lib/browser-storage'], function($, moment, eventBus,bstorage) {
    const windowTop = window.top;
    const sendEvent = (e)=>{
        e["ref"] = windowTop.location.href
        BidFlyerContext.postPageEvent(e);
    }
    let currentUrl = windowTop.location.origin + windowTop.location.pathname + windowTop.location.search;

    const postPageViewEvents = () => {
        const stepPath = windowTop.location.pathname.split('/');
        const step = stepPath.length < 3 || stepPath[stepPath.length -1] === "" ? 'main-page': stepPath[stepPath.length -1];
        const currency = (windowTop.localStorage.getItem("viva-user-currency") || ($('.country-flag', windowTop.document).next().text() == ''?undefined:$('.country-flag', windowTop.document).next().text()) || "MXN").toLowerCase()
        const language = (windowTop.localStorage.getItem("viva-locale") || window.top.location.href.split('/')[3] || "es-MX").toLowerCase()
        const resolution = window.top.innerWidth + "x" + window.top.innerHeight + ":"+window.top.screen.width + "x" + window.top.screen.height + ":" + window.top.devicePixelRatio.toFixed(2)
        let passengers = {"adults":0,"kids":0,"babies":0}
        let search = false;
        let origin = "";
        let destination = "";
        let startDate = "";
        let endDate = "";
        let tripType = "";
        let modality = "";
        try{
            passengers = JSON.parse(window.top.localStorage.getItem("passengers"))
        }catch(e){}
        if(windowTop.location.href.indexOf('book/options') > 10)
        {
            try{
                modality = windowTop.location.search.replace('?','').split('&').map(e=>{return{
                    "key":e.split('=')[0],
                    "val":e.split('=')[1]
                }}).find(e=>e.key=="modality").val
            }catch (e) {}

            try {
                search = JSON.parse(window.top.localStorage.getItem("viva-flights-search-history"))[0].content
                origin = search.origin.code
                destination = search.destination.code
                startDate = moment(search.startDate).format("YYYY-MM-DD")
                endDate = moment(search.endDate).format("YYYY-MM-DD")
                tripType = search.travelType
            }catch (e) {}

            BidFlyerContext.isHotelsAvailable().then((hasHotels)=>{
                sendEvent({
                    step: step,
                    a : passengers.adults,
                    k : passengers.kids,
                    i : passengers.babies,
                    l : language,
                    r : resolution,
                    c : currency,
                    s : startDate,
                    e : endDate,
                    t : tripType,
                    d : destination,
                    o : origin,
                    m : modality,
                    ha: !!hasHotels,
                    hs: $('.breadcrumb-item .fas.fa-bed', windowTop.document).length > 0,
                    typeOfEvent:"HotelsInFunnel"
                })


            });

        }
        else if(windowTop.location.href.indexOf('book/passenger') > 10  ||
            windowTop.location.href.indexOf('book/extras') > 10  ||
            windowTop.location.href.indexOf('book/seats') > 10 ||
            windowTop.location.href.indexOf('book/hotel') > 10 ||
            windowTop.location.href.indexOf('book/review') > 10 ||
            windowTop.location.href.indexOf('book/confirmation') > 10
        )
        {
            const basket = VivaContext.getBasketData();
            let basketPromise;
            if(typeof basket === 'object' && typeof basket.then === 'function')
            {
                basketPromise = basket
            }
            else
            {
                basketPromise = new Promise((resolve) => {
                    const basketInterval = window.setInterval(()=>{
                        const data = VivaContext.getBasketData()
                        if(data.destination) {
                            window.clearInterval(basketInterval)
                            resolve(data);
                        }
                    },500)
                })
            }
            basketPromise.then(data=>{
                try {
                    search = JSON.parse(window.top.localStorage.getItem("viva-flights-search-history"))[0].content
                    origin = search.origin.code
                    tripType = search.travelType
                }catch (e) {}

                BidFlyerContext.isHotelsAvailable().then((hasHotels)=>{
                    sendEvent({
                        step: step,
                        a : passengers.adults,
                        k : passengers.kids,
                        i : passengers.babies,
                        l : language,
                        r : resolution,
                        c : currency,
                        s : data.destination.checkIn,
                        e : data.destination.checkOut,
                        t : tripType,
                        d : data.destination.destination,
                        o : origin,
                        m : modality,
                        p : data.booking.pnr,
                        ps: data.payment.success,
                        ha: !!hasHotels,
                        hs: $('.breadcrumb-item .fas.fa-bed', windowTop.document).length > 0,
                        ho: data.hotel.bookingCode,
                        typeOfEvent:"HotelsInFunnel"
                    })
                })
            })
        }
        else {
            try {
                search = JSON.parse(window.top.localStorage.getItem("viva-flights-search-history"))[0].content
                origin = search.origin.code
                destination = search.destination.code
                startDate = moment(search.startDate).format("YYYY-MM-DD")
                endDate = moment(search.endDate).format("YYYY-MM-DD")
                tripType = search.travelType
            }catch (e) {}

            BidFlyerContext.isHotelsAvailable().then((hasHotels)=> {

                sendEvent({
                    step: step,
                    a: passengers.adults,
                    k: passengers.kids,
                    i: passengers.babies,
                    l: language,
                    r : resolution,
                    c: currency,
                    s: startDate,
                    e: endDate,
                    t: tripType,
                    d: destination,
                    o: origin,
                    m: modality,
                    ha: !!hasHotels,
                    hs: $('.breadcrumb-item .fas.fa-bed', windowTop.document).length > 0,
                    typeOfEvent: "HotelsInFunnel"
                })
            });
        }
    }

    if (windowTop.location.href.indexOf('book/passenger') > 10
        || windowTop.location.href.indexOf('book/extras') > 10
        || windowTop.location.href.indexOf('book/seats') > 10) {
        BidFlyerContext.searchHotels();
    }

    if (windowTop.location.href.indexOf('book/confirmation') > 10
        || windowTop.location.href.indexOf('manage/trip-details') > 10) {
        BidFlyerContext.startOrderStatusByPNR();
    }

    postPageViewEvents();

    window.setInterval(() => {
        if ((windowTop.location.origin + windowTop.location.pathname + windowTop.location.search) != currentUrl) {
            currentUrl = windowTop.location.origin + windowTop.location.pathname + windowTop.location.search;
            if (windowTop.location.href.indexOf('book/passenger') > 10
                || windowTop.location.href.indexOf('book/extras') > 10
                || windowTop.location.href.indexOf('book/seats') > 10) {
                BidFlyerContext.searchHotels();
            }

            if (windowTop.location.href.indexOf('book/confirmation') > 10) {
                BidFlyerContext.startPaymentProcess();
            }

            if (windowTop.location.href.indexOf('manage/trip-details') > 10) {
                BidFlyerContext.startOrderStatusByPNR();
            }

            if (windowTop.location.href.indexOf('book/') === -1 ) {
                BidFlyerContext.cleanSession();
            }

            postPageViewEvents();

        }
    }, 500)
}) } catch(e) { console.log(e)  }
				}
			}
			BidFlyerContext.execute(callbackGen(bfKey));

			onReady(function() {
				var serviceWorkerFrame = document.createElement("iframe");
				serviceWorkerFrame.id = "BFServiceWorker";
				serviceWorkerFrame.title = "Bidflyer Service Worker iframe";
				serviceWorkerFrame.style.width = 0;
				serviceWorkerFrame.style.height = 0;
				serviceWorkerFrame.width = 0;
				serviceWorkerFrame.height = 0;
                serviceWorkerFrame.style.position = "fixed";
                serviceWorkerFrame.style.left = "-9999px";
				serviceWorkerFrame.src = "https://vivaaerobus.app.bidflyer.com/deploy/serviceWorkerInstaller"
				serviceWorkerFrame.style.border = "none";
				serviceWorkerFrame.style.visibility = "hidden";
				document.body.appendChild(serviceWorkerFrame);

				iframe = document.createElement("iframe");
				iframe.id = "BFLoader";
		        iframe.title = "Bidflyer Loader Frame";
				iframe.width = 1;
				iframe.height = 1;
                iframe.style.width = "1px";
                iframe.style.height = "1px";
                iframe.style.position = "fixed";
                iframe.style.left = "-9999px";
				iframe.style.border = "none";
				iframe.style.visibility = "visible";
				document.body.appendChild(iframe);
