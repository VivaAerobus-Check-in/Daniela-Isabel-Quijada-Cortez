          label: "VL",
          search: {
            booking_window: 12,
            tab_type: "HEADER_BOOK_PACKAGES",
            type: "package",
            source: "home",
            total_guest_count: data.q.adultCount + data.q.childCount,
            guest: getPassengersTypes(data.q),
            view_type: "list_flights",
            search_using_points: "false",
            language: data.q.language,
            currency: data.q.currency,
            destination: data.flight.arriveCity,
            departure_date: data.q.checkOutDate,
            arrival_date: data.q.checkInDate,
            passenger_type_list: "",
            promo_code: "",
            rute_list: data.flight.departCity + "-" + data.flight.arriveCity + "|" + data.flight.arriveCity + "-" + data.flight.departCity,
            trip_type: "round-trip",
            origin: data.flight.departCity,
            availability: "",
            available_flights: data.q.numberOfFlights
          },
          ec: {
            items: [{
              item_name: "Modality Option",
              item_id: "VL|" + data.flight.departCity + "-" + data.flight.arriveCity + "|" + data.flight.departDay + 'T' + data.flight.departTime,
              price: "",
              currency: data.q.currency,
              item_brand: "Bidflyer",
              item_category: "Modality",
              item_category_2: "VL",
              item_category_3: "Domestic",
              item_category_4: "NonStop",
              item_variant: "VL",
              coupon: "",
              discount: "0.00",
              item_list_name: data.flight.departCity + "-" + data.flight.arriveCity + "|" + data.q.checkInDate,
              index: 1
            }]
          },
          ecommerce: {
            currencyCode: data.q.currency,
            click: {
              actionField: {
                list: data.flight.departCity + "-" + data.flight.arriveCity + "|" + data.q.checkInDate
              },
              products: {
                name: "Modality Option",
                id: data.flight.departCity + "-" + data.flight.arriveCity + "|" + data.flight.departDay + 'T' + data.flight.departTime,
                price: "",
                brand: "VB",
                category: "Modality/VZ/Domestic/NonStop",
                variant: "VL",
                coupon: "",
                position: 1
              }
            }
          },
          "gtm.uniqueEventId": 1201
        }

        pushGAEvent(dataToPush)
      }
    }

    if (eventName === "payment_failure") {
      if (eventDomain === "hotels" || eventDomain === "vacations") {
        let dataToPush = {
          event: "ev_payment_payment_attempt", category: "payment",
          action: "payment_attempt",
          label: "declined" //Could be accepted|declined
        }

        pushGAEvent(dataToPush)
      }
    }

    if (eventName === "payment_success") {
      if (eventDomain === "hotels" || eventDomain === "vacations") {
        let dataToPush = {
          event: "ev_payment_payment_attempt", category: "payment",
          action: "payment_attempt",
          label: "accepted" //Could be accepted|declined
        }

        pushGAEvent(dataToPush)
      }
    }

    if (eventName === "thanks_view") {
      if (eventDomain === "hotels") {
        let dataToPush = {
          event: "purchase_bidflyer",
          id: {
            reservation_id: data.o.order.id
          },
          transaction: {
            id: "T" + data.o.order.id,// Transaction ID
            flow_type: "Booking_hotel",
            trip_type: "RT",
            checkout_type: "normal",
            basket_amount: data.o.order.amount,
            currency: data.o.order.currency,
            total_room_count: 2, //New
            total_guest_count: 4, //New
            payments: [{
              status: "Approved",
              type: "CardPayment",
              installments: "",
              //code: "XH",
              //reference: "53406886",
              amount: {value: data.o.order.amount, currencyCode: data.o.order.currency}
            }],
            reserve: {
              id: data.o.order.id,
              creationTime: data.o.order.created,
              status: data.o.order.status
            },
          },
          itinerary: {
            destination: data.q.to.city + "," + data.q.to.country,
            departureDate: data.o.journey.filter(i => i.subtype == "hotel")[0].startingOn,
            arrivalDate: data.o.journey.filter(i => i.subtype == "hotel")[0].until,
            price: {
              currency: data.o.order.currency,
              room: {
                price: data.o.order.amount,
                quantity: data.o.journey.filter(i => i.subtype == "hotel")[0].hotel.rooms.length
              }
            },
          },
          ec: {
            transaction_id: "T" + data.o.order.id,// Transaction ID
            affiliation: "Payment_hotel",
            currency: data.o.order.currency,
            value: data.o.order.amount,
            //tax: 26.83,
            shipping: 0,
            coupon: "",
            items: [{
              item_name: "HOTEL: " + data.o.journey.filter(i => i.subtype == "hotel")[0].hotel.name,
              //item_id: "H:ABC123",// Should be the ID of the Hotel
              price: data.o.order.amount,
              currency: data.o.order.currency,
              item_brand: "BidFlyer",// Jessica conversation 12/04/22
              item_category: "Hotel",// Must be Hotel
              item_category_2: "Room", // Must be Room
              item_variant: data.o.journey.filter(i => i.subtype == "hotel")[0].hotel.rooms.map(r => r.type + "|" + r.title).join(","),//Should be room category|type
              coupon: "",
              discount: 0,
              quantity: data.o.journey.filter(i => i.subtype == "hotel")[0].hotel.rooms.length
            }]
          },
          ecommerce: {
            currencyCode: data.o.order.currency,
            purchase: {
              actionField: {
                //id: "T12345", // Transaction ID
                affiliation: "Payment_hotel",
                revenue: data.o.order.amount,

                //tax: 26.83,
                shipping: 0,
                coupon: ""
              },
              products: [{
                name: "HOTEL: " + data.o.journey.filter(i => i.subtype == "hotel")[0].hotel.name,
                //id: "H:ABC123",// Should be the ID of the Hotel
                price: data.o.order.amount,
                brand: "BidFlyer",// Jessica conversation 12/04/22
                category: "Hotel/Room",
                variant: data.o.journey.filter(i => i.subtype == "hotel")[0].hotel.rooms.map(r => r.type + "|" + r.title).join(","),//Should be room category|type
                coupon: ""
              }]
            }
          }
        }

        pushGAEvent(dataToPush)
      }
      if (eventDomain === "vacations") {
        let dataToPush = {
          event: "purchase_bidflyer",
          transaction: {
            id: data.o.order.id,// Transaction ID
            flow_type: "booking_package",
            trip_type: "OW",
            checkout_type: "normal",
            basket_amount: data.o.order.amount,
            currency: data.o.order.currency,
            rute_list: data.q.from.city + "-" + data.q.to.city,
            segment_list: data.q.from.city + "-" + data.q.to.city,
            total_room_count: data.o.journey.filter(x => x.subtype === "hotel")[0].hotel.rooms.length, //New
            total_guest_count: data.o.order.numOfTravellers, //New
            market: "domestic",
            modality: "VL",
            payments: [{
              status: "Approved",
              type: "CardPayment",
              installments: "",
              code: "VU",
              //reference: "53406886",
              amount: {
                value: data.o.order.amount,
                currencyCode: data.o.order.currency
              }
            }],
            reserve: {
              id: data.o.order.id,
              creationTime: data.o.order.created,
              status: "Paid",
              limitPayment: false
            },
          },
          ec: {
            currency: data.o.order.currency,
            value: data.o.order.amount,
            //tax: 26.83,
            shipping: 0,
            coupon: "",
            items: []
          },
          item_list_name: {
            affiliation: "Payment_booking",
            revenue: data.o.order.amount,
            //tax:76.13,
            shipping: 0,
            coupon: ""
          },
          ecommerce: {
            currencyCode: data.o.order.currency,
            purchase: {
              actionField: {
                affiliation: "Payment_booking",
                revenue: data.o.order.amount,
                //tax: 76.13,
                shipping: 0,
                coupon: ""
              },
              products: []
            }
          }
        }

        data.o.journey.filter(x => x.subtype === "hotel").map(item => {
          dataToPush.ec.items.push({
            item_name: item.hotel.name,
            item_id: "", //item_id from API
            price: data.o.order.amount, //price per night
            currency: data.o.order.currency,
            brand: "Bidflyer", //item brand if it exists, if not “Bidflyer” item_category: "Hotel",
            item_category: "Hotel",
            item_category_2: "Room",
            item_category_3: "Domestic", //Destiny type: Domestic, International item_variant: "Double Room / Deluxe",
            item_variant: item.hotel.rooms[0].type,
            coupon: "",
            discount: "0.00",
            quantity: 1,
            fare_class: "Z",
            loyalty_program: "",
            passenger_type: "ADT",
            passenger_count: data.o.order.numOfTravellers
          })
        })

        data.o.journey.filter(x => x.subtype === "flight").map(item => {
          dataToPush.ec.items.push({
            item_name: "Flight ticket",
            item_id: item.from.cityCode + "-" + item.to.cityCode + "|" + item.startingOn,
            price: data.o.order.amount,
            currency: "MXN",
            item_brand: "VB",
            item_category: "Flight",
            item_category_2: "VL",
            item_category_3: "Domestic",
            item_category_4: "NonStop",
            item_variant: "VL",
            coupon: "",
            discount: "0.00",
            quantity: 1,
            passenger_type: "ADT",
            passenger_count: 1
          })
        })

        data.o.journey.filter(x => x.subtype === "hotel").map(item => {
          dataToPush.ecommerce.purchase.products.push({
            item_name: item.hotel.name,
            price: data.o.order.amount, //price per night
            brand: "Bidflyer", //item brand if it exists, if not “Bidflyer” item_category: "Hotel",
            category: "Hotel",
            item_variant: item.hotel.rooms[0].type,
            promo: "",
            quantity: 1,
            fare_class: "Z",
            loyalty_program: "",
            passenger_type: "ADT",
            passenger_count: data.o.order.numOfTravellers
          })
        })

        data.o.journey.filter(x => x.subtype === "flight").map(item => {
          dataToPush.ecommerce.purchase.products.push({
            item_name: "Flight ticket",
            item_id: item.from.cityCode + "-" + item.to.cityCode + "|" + item.startingOn,
            price: data.o.order.amount,
            brand: "VB",
            category: "Flight",
            item_variant: "VL",
            coupon: "",
            promo: "0.00",
            quantity: 1,
            passenger_type: "ADT",
            passenger_count: 1
          })
        })

        pushGAEvent(dataToPush)
      }

    }

  } catch(e){
    console.error("failed to respond to event: ", arguments, e)
  }
}
